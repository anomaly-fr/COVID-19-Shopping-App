<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:layout_height="match_parent"
    android:layout_width="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".ui.ManageShop.ManageShopFragment">
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.ManageShop.ManageShopFragment"
    android:id="@+id/manageshop_framelayout"
    android:layout_above="@id/manageshop_bottom_navigation">
</FrameLayout>


    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/manageshop_bottom_navigation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:background="@color/colorPrimaryDark"
        app:itemIconTint="@drawable/bottomnavigation_onselect_colour"
        app:itemTextColor="@drawable/bottomnavigation_onselect_colour"
        app:menu="@menu/manageshop_bottom_navigation_menu"
        android:layout_alignParentBottom="true"/>

</RelativeLayout>

